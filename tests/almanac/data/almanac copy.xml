<almanac>
    <namespace prefix="small_sky">
        <catalog name="object" type="object" relative_path="small_sky/object"/>
        <catalog name="detections" type="source" relative_path="small_sky/detections/">
            <primary>object</primary>
        </catalog>
        <catalog name="object_neighbor_cache" type="neighbor" relative_path="small_sky/object_neighbor_cache/">
            <primary>object</primary>
        </catalog>
        <catalog name="small_sky_to_empty" type="association" relative_path="small_sky/small_sky_to_empty/">
            <primary>object</primary>
            <join>empty</join>
        </catalog>
        <catalog name="object_to_detections" type="association" relative_path="small_sky/object_to_detections/">
            <primary>object</primary>
            <join>detections</join>
        </catalog>
        <catalog name="object_id_index" type="index" relative_path="small_sky/object_id_index/">
            <primary>object</primary>
        </catalog>
    </namespace>
    <namespace  prefix="empty">
        <catalog name="empty" relative_path="empty/"/>
    </namespace>
</almanac>